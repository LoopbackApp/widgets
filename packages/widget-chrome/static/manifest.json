{
	"manifest_version": 3,
	"name": "Loopback",
	"version": "0.1.0",
	"description": "Collect Feedback and Share Updates All in One Place",
	"permissions": ["activeTab"],
	"action": {
		"default_popup": "index.html"
	},
	"icons": {
		"16": "images/logo-16.png",
		"32": "images/logo-32.png",
		"48": "images/logo-48.png",
		"128": "images/logo-128.png"
	}
}
